{
    "name": "phalconslayer/framework",
    "keywords": [
        "skeleton",
        "boilerplate",
        "framework",
        "phalcon",
        "slayer",
        "bootstrap"
    ],
    "description": "The foundation of Phalcon Slayer",
    "license": "MIT",
    "homepage": "http://github.com/phalconslayer/framework",
    "support": {
        "issues": "https://github.com/phalconslayer/framework/issues",
        "source": "https://github.com/phalconslayer/framework"
    },
    "authors": [
        {
            "name": "Daison Carino",
            "email": "daison12006013@gmail.com"
        }
    ],
    "require": {
        "classpreloader/classpreloader": "^2.0",
        "classpreloader/console": "^1.0",
        "fzaninotto/faker": "~1.4",
        "guzzlehttp/guzzle": "6.1.*",
        "illuminate/support": "5.2.*",
        "illuminate/view": "5.2.*",
        "paragonie/random_compat": "1.1.*",
        "league/flysystem": "^1.0",
        "league/tactician": "^0.6",
        "mailgun/mailgun-php": "~1.7.2",
        "monolog/monolog": "^1.15",
        "robmorgan/phinx": "0.5.*",
        "swiftmailer/swiftmailer": "^5.4",
        "symfony/console": "^2.7",
        "symfony/debug": "^2.7",
        "symfony/process": "^2.7",
        "symfony/var-dumper": "^2.7",
        "tijsverkoyen/css-to-inline-styles": "^1.5",
        "vlucas/phpdotenv": "^2.0",
        "respect/validation": "1.0.*"
    },
    "replace": {
        "clarity/contracts": "self.version",
        "clarity/kernel": "self.version",
        "clarity/support": "self.version",
        "clarity/exceptions": "self.version",
        "clarity/view": "self.version"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Clarity\\": "src/Clarity/"
        },
        "files": [
            "src/Clarity/Support/Helpers/init.php"
        ]
    }
}
